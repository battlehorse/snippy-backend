{
  "name": "Snippy",
  "version": "0.2.1",
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": "img/browser_action.png",
    "default_title": "Snippy",
    "popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "css": [],
      "js": ["lib/jquery-1.4.min.js", "js/snippy_selectables.js", "js/snippy.js"],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "background_page": "background.html",
  "permissions": [ "tabs", "http://snipbin.appspot.com/" ]
}
