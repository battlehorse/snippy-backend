{
  "name": "Snippy",
  "version": "0.1",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon32.png",
    "default_title": "Snippy",
    "popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*" ],
      "css": [ "snippy.css" ],
      "js": ["jquery-1.3.2.js", "snippy.js"],
      "run_at": "document_end"
    }
  ],
  "background_page": "background.html",
  "permissions": [ "tabs", "http://snipbin.appspot.com/" ]
}
